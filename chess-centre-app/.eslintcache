(error) {\r\n        dispatch({ type: \"LOGIN_ERROR\", error });\r\n      }\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div className=\"flex items-center min-h-screen p-6 bg-gray-50 dark:bg-gray-900\">\r\n      <div className=\"flex-1 h-full max-w-4xl mx-auto overflow-hidden bg-white rounded-lg shadow-xl dark:bg-gray-800\">\r\n        <div className=\"flex flex-col overflow-y-auto md:flex-row\">\r\n          <div className=\"h-32 md:h-auto md:w-1/2\">\r\n            <img\r\n              aria-hidden=\"true\"\r\n              className=\"object-cover w-full h-full dark:hidden\"\r\n              src={ImageLight}\r\n              alt=\"Office\"\r\n            />\r\n            <img\r\n              aria-hidden=\"true\"\r\n              className=\"hidden object-cover w-full h-full dark:block\"\r\n              src={ImageDark}\r\n              alt=\"Office\"\r\n            />\r\n          </div>\r\n          <main className=\"flex items-center justify-center p-6 sm:p-12 md:w-1/2\">\r\n            <div className=\"w-full\">\r\n              {errorMessage && <p style={{ color: \"red\" }}>{errorMessage}</p>}\r\n              <img src={Logo} className=\"object-contain h-48 w-full\" alt=\"The Chess Centre\" />\r\n              <h1 className=\"mb-4 text-xl font-semibold text-gray-700 dark:text-gray-200\">\r\n                Create account\r\n              </h1>\r\n              <Label>\r\n                <span>Email</span>\r\n                <Input disabled={loading}\r\n                  value={email}\r\n                  onChange={(e) => setEmail(e.target.value)}\r\n                  className=\"mt-1\" type=\"email\" placeholder=\"john@doe.com\" />\r\n              </Label>\r\n              <Label className=\"mt-4\">\r\n                <span>Password</span>\r\n                <Input disabled={loading}\r\n                  value={password}\r\n                  onChange={(e) => setPassword(e.target.value)} className=\"mt-1\"\r\n                  placeholder=\"***************\" type=\"password\" />\r\n              </Label>\r\n              <Label className=\"mt-4\">\r\n                <span>Confirm password</span>\r\n                <Input disabled={loading}\r\n                  value={rePassword}\r\n                  className=\"mt-1\"\r\n                  onChange={(e) => setRePassword(e.target.value)}\r\n                  placeholder=\"***************\"\r\n                  type=\"password\" />\r\n              </Label>\r\n\r\n              <Label className=\"mt-6\" check>\r\n                <Input type=\"checkbox\" checked={ischecked} disabled={loading}\r\n                  onChange={(e) => setischecked(e.target.checked)} />\r\n                <span className=\"ml-2\">\r\n                  I agree to the <span className=\"underline\">privacy policy</span>\r\n                </span>\r\n              </Label>\r\n\r\n              <Button onClick={signUp} disabled={isButton && \"disabled\"} block className=\"mt-4\">\r\n                Create account\r\n              </Button>\r\n\r\n              <hr className=\"my-8\" />\r\n\r\n              {/* <Button block layout=\"outline\">\r\n                <GithubIcon className=\"w-4 h-4 mr-2\" aria-hidden=\"true\" />\r\n                Github\r\n              </Button> */}\r\n\r\n              <p className=\"mt-4\">\r\n                <Link\r\n                  className=\"text-sm font-medium text-teal-600 dark:text-teal-400 hover:underline\"\r\n                  to=\"/login\"\r\n                >\r\n                  Already have an account? Login\r\n                </Link>\r\n              </p>\r\n            </div>\r\n            <PrivacyPolicyModal></PrivacyPolicyModal>\r\n          </main>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Login\r\n","C:\\Users\\user\\develop\\platform\\chess-centre-app\\src\\pages\\Auth\\ForgotPassword.js",[],"C:\\Users\\user\\develop\\platform\\chess-centre-app\\src\\pages\\Home\\LandingPage.js",[],"C:\\Users\\user\\develop\